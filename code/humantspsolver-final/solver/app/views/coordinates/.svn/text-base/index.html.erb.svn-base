<h2>Problem Coordinates: <%= @problem.name %></h2>

<table width="100%">
  <tr>
    <th>City</th>
    <th>X</th>
    <th>Y</th>
  </tr>

<% for coordinate in @coordinates %>
  <tr align="center">
    <td><%=h coordinate.city %></td>
    <td><%=h coordinate.x %></td>
    <td><%=h coordinate.y %></td>
  </tr>
<% end %>
</table>

<br/>
<div align="center"> 
	<%= link_to 'Problem Details', @problem %>
</div>

